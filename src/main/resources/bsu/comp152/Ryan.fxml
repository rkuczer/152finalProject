<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bsu.comp152.ryanController">
   <children>
      <VBox layoutX="-1.0" layoutY="-3.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <TextArea prefHeight="75.0" prefWidth="600.0" text="Chuck Norris Jokes">
               <font>
                  <Font name="Wide Latin" size="30.0" />
               </font>
            </TextArea>
            <TextField text="Joke Categories: animal, career, celebreity, dev, fashion, food, history, money, movie, music." />
            <TextField fx:id="JokePrompt" prefHeight="25.0" prefWidth="114.0" text="Enter the category of a joke below.">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TextField>
            <TextField fx:id="jokeInput" />
            <Button mnemonicParsing="false" onAction="#loadData" prefHeight="25.0" prefWidth="163.0" text="Confirm Category Selection" />
         </children>
      </VBox>
   </children>
</AnchorPane>
