<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bsu.comp152.ryanController">
   <children>
      <VBox layoutX="-1.0" layoutY="-3.0" prefHeight="87.0" prefWidth="600.0">
         <children>
            <TextArea prefHeight="75.0" prefWidth="600.0" text="Chuck Norris Jokes">
               <font>
                  <Font name="Wide Latin" size="30.0" />
               </font>
            </TextArea>
            <TextField text="Joke Categories: animal, career, celebreity, dev, fashion, food, history, money, movie, music." />
            <TextField fx:id="JokePrompt" prefHeight="25.0" prefWidth="114.0" text="Enter the category of a joke below.">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TextField>
            <ListView fx:id="ListControl" prefHeight="200.0" prefWidth="200.0" />
         </children>
      </VBox>
      <VBox layoutX="169.0" layoutY="130.0" prefHeight="200.0" prefWidth="181.0">
         <children>
            <CheckBox mnemonicParsing="false" text="animal" />
            <CheckBox mnemonicParsing="false" text="career" />
            <CheckBox mnemonicParsing="false" text="celebrity" />
            <CheckBox mnemonicParsing="false" text="dev" />
            <CheckBox mnemonicParsing="false" text="fashion" />
            <CheckBox mnemonicParsing="false" text="food" />
            <CheckBox mnemonicParsing="false" text="history" />
            <CheckBox mnemonicParsing="false" text="money" />
            <CheckBox mnemonicParsing="false" text="movie" />
            <CheckBox mnemonicParsing="false" text="music" />
         </children></VBox>
      <VBox layoutX="-1.0" layoutY="130.0" prefHeight="200.0" prefWidth="181.0">
         <children>
            <Button fx:id="jokeButton" mnemonicParsing="false" onAction="#loadData" prefHeight="64.0" prefWidth="100.0" text="Get Joke" />
            <MenuButton mnemonicParsing="false" text="MenuButton">
              <items>
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="animal" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="career" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="celebrity" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="dev" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="fashion" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="food" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="history" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="money" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="movie" />
                 <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="music" />
              </items>
            </MenuButton>
         </children>
      </VBox>
      <VBox fx:id="ListControl" layoutX="350.0" layoutY="130.0" prefHeight="270.0" prefWidth="250.0">
         <children>
            <ListView fx:id="ListControl" prefHeight="270.0" prefWidth="250.0" />
         </children></VBox>
   </children>
</AnchorPane>
